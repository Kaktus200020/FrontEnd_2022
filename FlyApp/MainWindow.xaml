<Window x:Class="FlyApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlyApp"
        mc:Ignorable="d"
        Title="FlyApp" Height="450" Width="800" FocusManager.FocusedElement="{Binding ElementName=MyCanvas}">
    <Canvas Name="MyCanvas" Focusable="True" MouseLeftButtonDown="MyCanvas_MouseLeftButtonDown" Background="White">
        <Ellipse Width="50" Height="50" Fill="Gray" Canvas.Left="375" Canvas.Top="175" Name="fly"/>
        <Label Name="lblScore" Canvas.Top="10" Content="SCORE" Canvas.Left="10"/>
    </Canvas>
</Window>
